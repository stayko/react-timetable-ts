{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es5",
    "removeComments": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
   ]
}
